        <script>
            document.addEventListener("DOMContentLoaded", () => {
                // Initialize modal functionality
                initializeModal('.js-btn', '.modal', '.close');
                initializeModal('.js-print-btn', '#printDetailsModal', '.close-btn');

                // Set the date of order field
                setDateInput('#date-of-order');

                // Fetch measurements on page load
                GetMeasurements();
            });

            // Function to initialize modal open and close functionality
            function initializeModal(buttonSelector, modalSelector, closeSelector) {
                const button = document.querySelector(buttonSelector);
                const modal = document.querySelector(modalSelector);
                const closeBtn = document.querySelector(closeSelector);

                button.addEventListener("click", () => {
                    modal.style.display = "block";
                });

                closeBtn.addEventListener("click", () => {
                    modal.style.display = "none";
                });

                window.addEventListener("click", (event) => {
                    if (event.target === modal) {
                        modal.style.display = "none";
                    }
                });
            }

            // Function to set the current date on a date input field
            function setDateInput(dateInputSelector) {
                const today = new Date();
                const dateInput = document.querySelector(dateInputSelector);

                const formattedDate = today.toISOString().split('T')[0];
                dateInput.value = formattedDate;
            }

            // Function to print the invoice
            function printInvoice() {
                window.print();
            }

            // Function to add a new measurement
            function AddMeasurement() {
                const formData = {
                    FirstName: $('#first-name').val(),
                    LastName: $('#last-name').val(),
                    Address: $('#address').val(),
                    DateOfOrder: $('#date-of-order').val(),
                    ContactNumber: $('#contact-number').val(),
                    BillingDetails: $('#billing-details').val(),
                    MeasurementDetails: $('#measurement-details').val()
                };

                console.log('Sending data:', formData);

                $.ajax({
                    url: '/Home/AddMeasurement',
                    type: 'POST',
                    contentType: 'application/json',
                    data: JSON.stringify(formData),
                    success: function (response) {
                        console.log('Server response:', response);
                        alert(response);
                        if (response === "Measurement added Successfully.") {
                            GetMeasurements();
                            $('.modal').hide(); // Close modal after success
                        }
                    },
                    error: function (xhr, status, error) {
                        console.error('AJAX Error:', error);
                        alert('Error: ' + error);
                    }
                });
            }

            // Function to fetch and display measurements
            function GetMeasurements() {
                $.ajax({
                    url: '/Home/GetMeasurements',
                    type: 'GET',
                    dataType: 'json',
                    success: function (data) {
                        console.log('Received data:', data);
                        if (Array.isArray(data)) {
                            updateTable(data);
                        } else {
                            console.error('Data is not an array:', data);
                            alert('Error: Received data is not an array.');
                        }
                    },
                    error: function (xhr, status, error) {
                        console.error('AJAX Error:', error);
                        alert('Error: ' + error);
                    }
                });
            }

            // Function to update the table with fetched measurements
            function updateTable(measurements) {
                const tbody = $('.table tbody');
                tbody.empty(); // Clear existing rows

                // Add new rows to the table
                measurements.forEach(function (item, index) {
                    const row = `
                                <tr>
                                    <td>${index + 1}</td>
                                    <td><img src="path/to/image" alt="Image"></td>
                                    <td>${item.FirstName} ${item.LastName}</td>
                                    <td>${index || 0}</td>
                                    <td>${item.MeasurementDetails}</td>
                                    <td>${item.BillingDetails}</td>
                                    <td>${item.DateOfOrder}</td>
                                    <td>${item.ContactNumber}</td>
                                    <td>${item.Status || 'N/A'}</td>
                                </tr>`;
                    tbody.append(row);
                });
            }
        </script>

        @*$(document).ready(function () {
        GetMeasurements(); // Fetch measurements when the page loads
        });*@

</body>

